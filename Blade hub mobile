game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Store"):WaitForChild("RequestOpenExplosionBox"):InvokeServer()
    print("Clicked")
end)

Section:NewToggle("Auto Open Chest Explosion", "?", function(state)
    if state then
        _G.Enabled = true

while _G.Enabled == true do
   wait()
   game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Store"):WaitForChild("RequestOpenExplosionBox"):InvokeServer()

end
    else
        _G.Enabled = false

while _G.Enabled == false do
   wait()
   game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Store"):WaitForChild("RequestOpenExplosionBox"):InvokeServer()

end
    end
end)

Section:NewToggle("Auto Open Chest Sword", "?", function(state)
    if state then
        _G.Enabled = true

while _G.Enabled == true do
   wait()
   game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Store"):WaitForChild("RequestOpenSwordBox"):InvokeServer()

end
    else
        _G.Enabled = false

while _G.Enabled == false do
   wait()
   game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Store"):WaitForChild("RequestOpenSwordBox"):InvokeServer()

end
    end
end) 

