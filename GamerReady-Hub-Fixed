--Values
_G.AutoRoom = true
_G.AutoText = true
_G.AutoClaimSpin = true
_G.AutoWalk = true


--functions

function AutoWalk()
while _G.AutoWalk == true do

local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
local targetPlayer = nil

-- FunciÃ³n para seleccionar un jugador aleatorio en el servidor
local function selectRandomPlayer()
    local players = game.Players:GetPlayers()
    local randomIndex = math.random(1, #players)
    return players[randomIndex]
end

-- FunciÃ³n para hacer que el personaje camine hacia un jugador
local function walkToPlayer(player)
    local targetCharacter = player.Character
    if targetCharacter then
        humanoid:MoveTo(targetCharacter.HumanoidRootPart.Position)
    end
end

-- FunciÃ³n para hacer que el personaje salte
local function jump()
    humanoid.Jump = true
end

-- Detectar cuando el jugador se sienta
humanoid.Seated:Connect(function(isSeated)
    if isSeated then
        jump()
    end
end)

-- Seleccionar un jugador aleatorio
targetPlayer = selectRandomPlayer()
walkToPlayer(targetPlayer)
   wait(5)
   wait(.0001)
 end
end

function AutoRoom()
while _G.AutoRoom == true do
   game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.Tickets.CreateRoom:InvokeServer(GameMode,Pricehot,{["assetType"] = "GamePass",["assetId"] = Room})
   wait(.0001)
 end
end

function AutoText()
while _G.AutoText == true do
   game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(CustomHot,"All")
   wait(15)
   wait(.0001)
 end
end

function AutoClaimSpin()
while _G.AutoClaimSpin == true do
   wait(10)
   game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.Tickets.DestroyRoom:InvokeServer()
   game:GetService("ReplicatedStorage").RemoteCalls.GameSpecific.DailySpinner.ClaimDailySpinner:InvokeServer()
   wait(.0001)
 end
end

