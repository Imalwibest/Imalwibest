                        plr.Character.Humanoid:EquipTool(k)
                    end 
                end
            elseif (not plr.Character:FindFirstChildOfClass("Tool")) then 
                for o,k in pairs(plr.Backpack:GetChildren()) do 
                    if string.find(k.Name, "Sword") then 
                        plr.Character.Humanoid:EquipTool(k)
                    end 
                end
            end 
        end 
    end 
end)

