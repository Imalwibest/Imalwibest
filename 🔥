local Library =loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("play for ugc", "Serpent")
local Main = Window:NewTab("Main")
local Section = Main:NewSection("farm")
Section:NewButton("Auto Jump", "?", function()
loadstring(game:HttpGet('https://pastebin.com/raw/vViq08jX'))()
end)

Section:NewButton("Dont move", "?", function()
local args = {
    [1] = "Freeze",
    [2] = 99999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("ForServer"):WaitForChild("TaskProgress"):FireServer(unpack(args))
end)

Section:NewButton("Auto Walk", "?", function()
local args = {
    [1] = 10000
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("ForServer"):WaitForChild("UpdQuest4"):FireServer(unpack(args))
end)

Section:NewButton("Auto Click", "?", function()
for i = 1,100000 do
local args = {
    [1] = "Click"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("ForServer"):WaitForChild("TaskProgress"):FireServer(unpack(args))
end
end)
