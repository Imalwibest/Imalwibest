   wait()
   game.Players.LocalPlayer.Character:PivotTo(CFrame.new(-34.3754959, 735.631897, 11, 0, 0, 1, 0, 1, 0, -1, 0, 0))
    
end
    else
        _G.Enabled = false

while _G.Enabled == false do
   wait()
   -- script here
end
    end
